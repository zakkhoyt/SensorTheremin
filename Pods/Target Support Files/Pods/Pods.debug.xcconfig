CLANG_MODULES_AUTOLINK = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AudioKit" "${PODS_ROOT}/Headers/Public/GPUImage" "${PODS_ROOT}/Headers/Public/VWWPermissionKit"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/AudioKit/AudioKit/Platforms/iOS/libs" "$(PODS_ROOT)/AudioKit/AudioKit/Platforms/iOS/libs"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AudioKit" -isystem "${PODS_ROOT}/Headers/Public/GPUImage" -isystem "${PODS_ROOT}/Headers/Public/VWWPermissionKit"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AudioKit" -l"GPUImage" -l"VWWPermissionKit" -l"csound" -l"sndfile" -framework "AVFoundation" -framework "CoreMedia" -framework "OpenGLES" -framework "QuartzCore"
PODS_ROOT = ${SRCROOT}/Pods